{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Library TypeScript Config",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "declaration": true,
    "declarationMap": true,
    "noEmit": true
  }
}
